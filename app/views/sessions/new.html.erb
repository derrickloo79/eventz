<h1>Sign In</h1>

<%= form_with(url: session_path) do |f| %>

    <%= f.label :email_username, "Email/Username" %>
    <%= f.text_field :email_username, autofocus: true, required: true %>

    <%= f.label :password %>
    <%= f.password_field :password, required: true %>

    <%= f.submit "Sign In" %>
<% end %>